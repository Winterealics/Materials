    "import csv\n",
    "import statistics as stat\n",
    "\n",
    "def dailyRate(p1, p2):\n",
    "    return (p2 - p1) / p1\n",
    "\n",
    "def rateList(l):\n",
    "    return [dailyRate(l[i], l[i + 1]) for i, c in enumerate(l[:-1])]\n",
