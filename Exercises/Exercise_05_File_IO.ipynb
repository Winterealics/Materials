    "#Exercise 5.7\n",
    "import math\n",
    "\n",
    "with open('./resources/primes1000000.txt', 'r') as f:\n",
    "    read = f.readlines()\n",
    "    primesList = [int(c.strip()) for c in read]\n",
    "\n",
    "def primeCheck(n):\n",
    "\n",
    "    if n < 1000000:\n",
    "        return n in primesList\n",
    "\n",
    "    else:\n",
    "        i = 0\n",
    "\n",
    "        while primesList[i] <= math.sqrt(n):\n",
    "            \n",
    "            if n % primesList[i] == 0:\n",
    "                return False\n",
    "            \n",
    "            i += 1\n",
    "\n",
    "        return True\n",
    "\n",
    "primeCheck(1000033)"
